			<!-- header-section start -->
			{% block header %}
				<header class="header-section">
					<div class="overlay">
						<div class="container">
							<div class="row d-flex header-area">
								<nav class="navbar navbar-expand-lg navbar-light">
									<a class="navbar-brand" href="{{path('home')}}">
										<img src="{{ asset('front-office/assets/images/calometre.png') }}" class="logo" alt="logo">
									</a>
									<button class="navbar-toggler collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#navbar-content">
										<i class="fas fa-bars"></i>
									</button>
									<div class="collapse navbar-collapse justify-content-end" id="navbar-content">
										<ul class="navbar-nav mr-auto mb-2 mb-lg-0">
<li class="nav-item">
	<a class="nav-link " aria-current="page" href="{{path('home')}}">Accueil</a>

</li>

<li class="nav-item">
	<a class="nav-link" href="{{ path('app_alim_front') }}">Aliments</a>
</li>
<li class="nav-item">
	<a class="nav-link" href="{{ path('exercice_front') }}">Exercices</a>
</li>
<li class="nav-item">
	<a class="nav-link" href="{{ path('event_client_index') }}">Evènements</a>
</li>
<li class="nav-item">
	<a class="nav-link" href="{{ path('product_front_index') }}">Produits</a>
</li>
<li class="nav-item">
	<a class="nav-link" href="{{ path('reclamation_new') }}">Réclamations</a>
</li>

											{% if app.user != null %}
												{% if app.user.roles|join(', ') =="ROLE_ADMIN" %}
													<li class="nav-item">
														<a class="nav-link" href="{{ path('users_list') }}">Admin Dashboard</a>
													</li>
												{% endif %}
											{% endif %}
										</ul>
										<div class="right-area header-action d-flex align-items-center max-un">
											{% if app.user %}
												<a href="{{ path('user') }}" style="color:white" type="button">
													{{ app.user.firstname }}
												</a>

												<a href="{{ path('app_logout') }}" type="button" class="cmn-btn reg">
													Logout
												</a>
											{% else %}


												<button type="button" class="login" data-bs-toggle="modal" data-bs-target="#loginMod">
													Se connecter
												</button>
												<a href="{{ path('app_register')}}" type="button" class="cmn-btn reg">
													S'inscrire
												</a>
											{% endif %}
										</div>
									</div>
								</nav>
							</div>
						</div>
					</div>
				</header>

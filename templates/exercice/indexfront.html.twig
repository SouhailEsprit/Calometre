 {% extends 'base-front-office.html.twig' %}



{% block mainContent %}
         <style>
         .countercart{
 width: 100%;
 display: flex;
 justify-content: space-between;
 align-items: center;
}
         .countcart{
 font-size: 20px;
 font-family: ‘Open Sans’;
 font-weight: 900;
 color: #202020;
}
         .btncart{
 width: 40px;
 height: 40px;
 border-radius: 50%;
 background-color: #d9d9d9;
 display: flex;
 justify-content: center;
 align-items: center;
 font-size: 20px;
 font-family: ‘Open Sans’;
 font-weight: 900;
 color: #202020;
 cursor: pointer;
}
         .pad-top-120 {
  padding-top: 20px;
}
.ab-section .overlay {
  background-color: #1F2340;
  background-image: src="{{ asset('front-office/assets/images/abcdef.png')}}"
  background-repeat: no-repeat;
  background-position: top 260px right 10px, top 200px right 0;
  position: relative;
  background-size: auto;
}
</style>
<body>
    <section class="banner-section inner-banner tournaments">
        <div class="overlay">
           
             
           
            <div class="ab-content">
  
<img src="{{ asset('front-office/assets/images/abcdef.png')}}" class="obj-8" alt="image">
      
 
  
            
        </div>
    </section>

    <!-- products section start -->
    <section class="tournaments-section">
        <div class="overlay pad-top-120">
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-lg-5">
                        <div class="section-header text-center">
                            
                            <h2 class="title">Our Exercices</h2>
                           
                        </div>
                    </div>
                </div>
                <div class="d-flex justify-content-center">
                    <ul class="nav" role="tablist">
                        <li class="nav-item" role="presentation">
                            <button class="cmn-btn active" id="ongoing-tab" data-bs-toggle="tab"
                                data-bs-target="#ongoing" type="button" role="tab" aria-controls="ongoing"
                                aria-selected="true">Cardio</button>
                        </li>
                        <li class="nav-item" role="presentation">
                            <button class="cmn-btn" id="upcoming-tab" data-bs-toggle="tab" data-bs-target="#upcoming"
                                type="button" role="tab" aria-controls="upcoming"
                                aria-selected="false">Musculation</button>
                        </li>
                        <li class="nav-item" role="presentation">
                            <button class="cmn-btn" id="past-tab" data-bs-toggle="tab" data-bs-target="#past"
                                type="button" role="tab" aria-controls="past" aria-selected="false">étirement</button>
                        </li>
                    </ul>
                </div>
                
                 
                <div class="tab-content">
     <div class="tab-pane fade show active" id="ongoing" role="tabpanel" aria-labelledby="ongoing-tab">
                  
                        <div class="row">
                            <div class="col-12">
                            {% for exercice in exercices %} 
                         {% if exercice.nomtype == "cardio" %}
                                <div class="single-item">
                                    <div class="left-item">
                                        <div class="logo-item">
                                        {% for video in exercice.video %}
                                            <div class="carousel-item {{ (loop.first) ? 'active' : '' }}">
			                            <video style= "border: 5px solid #555;" src="{{ asset('/uploads/exercices/' ~ video) }}" class="d-block w-100" alt="icon" type="video/mp4">
	                                    </div>
                                        {% endfor %}
                                        </div>
                                        <div class="mid-area">
                                            <h3>{{ exercice.nom }}</h3>
                                            
                                            <p>Description:</p>
                                            <p><i>{{ exercice.description }}</i></p>
                                            <p>Objectif:</p>
                                            <p><i>{{ exercice.objectif }}</i></p>
                                        </div>
                                    </div>
                                    <div class="last-item">
                                         <video width="100" controls style="width: 536px;">
                                        <source src="{{ asset('/uploads/exercices/' ~ exercice.video) }}" type="video/mp4">
  
                                        </video>
                                            </div>
                                       
                                      
                                    </div>
                                </div>
                                {% endif %}
                                {% endfor %}
                            </div>
                        </div>
                        <div class="row mt-60">
                            <div class="col-lg-12 d-flex justify-content-center">
                                <nav aria-label="Page navigation" class="d-flex justify-content-center">
                                    <ul class="pagination justify-content-center align-items-center">
                                        <li class="page-item">
                                            <a class="page-btn previous" href="javascript:void(0)"
                                                aria-label="Previous">
                                                <img src="{{ asset('front-office/assets/images/icon/arrow-left.png')}}" alt="icon">
                                            </a>
                                        </li>
                                        <li class="page-item"><a class="page-link xlr" href="javascript:void(0)">1</a>
                                        </li>
                                        <li class="page-item"><a class="page-link xlr active"
                                                href="javascript:void(0)">2</a></li>
                                        <li class="page-item"><a class="page-link xlr" href="javascript:void(0)">3</a>
                                        </li>
                                        <li class="page-item"><a class="page-link xlr" href="javascript:void(0)">4</a>
                                        </li>
                                        <li class="page-item">
                                            <a class="page-btn next" href="javascript:void(0)" aria-label="Next">
                                                <img src="{{ asset('front-office/assets/images/icon/arrow-right.png')}}" alt="icon">
                                            </a>
                                        </li>
                                    </ul>
                                </nav>
                            </div>
                        </div>
                    </div>
                    
                    <div class="tab-pane fade" id="upcoming" role="tabpanel" aria-labelledby="upcoming-tab">
                    
                        <div class="row">
                            <div class="col-12">
                                 {% for exercice in exercices %} 
                         {% if exercice.nomtype == "musculation" %}
                                <div class="single-item">
                                    <div class="left-item">
                                        <div class="logo-item">
                                            {% for video in exercice.video %}
                                            <div class="carousel-item {{ (loop.first) ? 'active' : '' }}">
			                            <video style= "border: 5px solid #555;" src="{{ asset('/uploads/exercices/' ~ video) }}" class="d-block w-100" alt="icon">
	                                    </div>
                                        {% endfor %}
                                        </div>
                                               <div class="mid-area">
                                            <h3>{{ exercices.nom }}</h3>
                                            
                                            <p><i>{{ exercices.description }}</i></p>
                                        </div>
                                    </div>
                                    <div class="last-item">
                                        <h6>objectif</h6>
                                        <h4>${{ exercice.objectif }}</h4>
                                        <span class="btn-border">
                                         
                                            <a href="tournaments-details.html" class="cmn-btn" >Add To Cart</a>
                                        </span>
                                    </div>
                                </div>
                                {% endif %}
                                {% endfor %}
                            </div>
                        </div>
                        <div class="row mt-60">
                            <div class="col-lg-12 d-flex justify-content-center">
                                <nav aria-label="Page navigation" class="d-flex justify-content-center">
                                    <ul class="pagination justify-content-center align-items-center">
                                        <li class="page-item">
                                            <a class="page-btn previous" href="javascript:void(0)"
                                                aria-label="Previous">
                                                <img src="{{ asset('front-office/assets/images/icon/arrow-left.png')}}" alt="icon">
                                            </a>
                                        </li>
                                        <li class="page-item"><a class="page-link xlr" href="javascript:void(0)">1</a>

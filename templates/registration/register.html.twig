{% block header %}
    <header class="header-section">
        <div class="overlay">
            <div class="container">
                <div class="row d-flex header-area">
                    <nav class="navbar navbar-expand-lg navbar-light">
                        <a class="navbar-brand" href="index.html">
                            <img src="{{ asset('front-office/assets/images/calometre.png') }}" class="logo" alt="logo">
                        </a>
                        <button class="navbar-toggler collapsed" type="button" data-bs-toggle="collapse"
                                data-bs-target="#navbar-content">
                            <i class="fas fa-bars"></i>
                        </button>
                        <div class="collapse navbar-collapse justify-content-end" id="navbar-content">
                            <ul class="navbar-nav mr-auto mb-2 mb-lg-0">
<li class="nav-item">
	<a class="nav-link " aria-current="page" href="{{path('home')}}">Accueil</a>

</li>

<li class="nav-item">
	<a class="nav-link" href="{{ path('app_alim_front') }}">Aliments</a>
</li>
<li class="nav-item">
	<a class="nav-link" href="{{ path('exercice_front') }}">Exercices</a>
</li>
<li class="nav-item">
	<a class="nav-link" href="{{ path('event_client_index') }}">Evenements</a>
</li>
<li class="nav-item">
	<a class="nav-link" href="{{ path('product_front_index') }}">Produits</a>
</li>
<li class="nav-item">
	<a class="nav-link" href="{{ path('reclamation_new') }}">Réclamations</a>
</li>

                            </ul>
                            <div class="right-area header-action d-flex align-items-center max-un">
                                {% if app.user %}
                                    <a href="{{path('user')}}" type="button" >
                                        {{app.user.firstname}}
                                    </a>
                                    <a href="{{ path('app_logout') }}" type="button" class="cmn-btn reg" >
                                        Logout
                                    </a>
                                {% else %}


                                    <a href="{{ path('app_login')}}" type="button" class="login" >
                                        Se connecter
                                    </a>
                                    <button type="button" class="cmn-btn reg" data-bs-toggle="modal"
                                            data-bs-target="#loginMod">
                                        S'inscrire
                                    </button>
                                {% endif %}
                            </div>
                        </div>
                    </nav>
                </div>
            </div>
        </div>

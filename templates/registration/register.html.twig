<!-- Login Registration start -->

<div class="log-reg">
    <div class="container">
        <div class="row">
            <div class="col-lg-6">
                <div class="modal fade" id="loginMod">

                    <div class="modal-dialog modal-dialog-centered">
                        <div class="modal-content">
                            <div class="modal-header justify-content-center">
                                <button type="button" class="btn-close" data-bs-dismiss="modal"
                                        aria-label="Close"></button>
                            </div>
                            <ul class="nav log-reg-btn justify-content-around">
                                <li class="bottom-area" role="presentation">
                                    <button class="nav-link" id="regArea-tab" data-bs-toggle="tab"
                                            data-bs-target="#regArea" type="button" role="tab" aria-controls="regArea"
                                            aria-selected="false">
                                         S'inscrire
                                    </button>
                                </li>
                                         
                            </ul>
                            <div class="tab-content">
                                <div class="tab-pane fade show active" id="loginArea" role="tabpanel"
                                     aria-labelledby="loginArea-tab">
                                    <div class="login-reg-content">
                                        <div class="modal-body">
                                            <div class="head-area">
                                                <h6 class="title">Login Direetly With</h6>
                                                <div class="social-link d-flex align-items-center">
                                                    <a href="javascript:void(0)" class="active"><i
                                                                class="fab fa-facebook-f"></i></a>
                                                    <a href="javascript:void(0)"><i class="fab fa-twitter"></i></a>
                                                    <a href="javascript:void(0)"><i
                                                                class="fab fa-linkedin-in"></i></a>
                                                    <a href="javascript:void(0)"><i
                                                                class="fab fa-instagram"></i></a>
                                                </div>
                                            </div>
                                            <div class="form-area">
                                                <div class="bottom-area text-center">
                                                    <p>Déja membre ? <a href="javascript:void(0)" class="reg-btn">S'inscrire</a></p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>



                                </div>

                                <div class="tab-pane fade" id="regArea" role="tabpanel"
                                     aria-labelledby="regArea-tab">
                                    <div class="login-reg-content regMode">
                                        <div class="modal-body">
                                            <div class="head-area">
                                                <h6 class="title">S'inscrire sur calometre</h6>
                                                <div class="social-link d-flex align-items-center">
                                                    <a href="javascript:void(0)" class="active"><i
                                                                class="fab fa-facebook-f"></i></a>
                                                    <a href="javascript:void(0)"><i class="fab fa-twitter"></i></a>
                                                    <a href="javascript:void(0)"><i
                                                                class="fab fa-linkedin-in"></i></a>
                                                    <a href="javascript:void(0)"><i
                                                                class="fab fa-instagram"></i></a>
                                                </div>
                                            </div>

                                            <div class="form-area">
                                                {{ form_start(registrationForm )}}
                                                <div class="row">
                                                    <div class="col-12">
                                                        <div class="single-input">

{{ form_row(registrationForm.firstname,{'attr':{'placeholder':'Nom'}}, {label: 'Nom'} ) }}

                                                        </div>
                                                        <div class="single-input">

                                                            {{ form_row(registrationForm.lastname,{'attr':{'placeholder':'Prénom'}}, {label: 'Prénom'} ) }}
                                                        </div>
                                                        <div class="single-input">

                                                            {{ form_row(registrationForm.email,{'attr':{'placeholder':'E-mail'}}, {label: 'E-mail'} ) }}
                                                        </div>
                                                        <div class="single-input">

                                                            {{ form_row(registrationForm.plainPassword,{'attr':{'placeholder':'Mot de passe'}}, {label: 'Mot de passe'} ) }}
                                                        </div>
                                                                                                                   
                                                        <div class="single-input">
{{ form_row(registrationForm.phonenumber,{'attr':{'placeholder':'Numéro de téléphone'}}, {label: 'Numéro de téléphone'} ) }}

                                                        </div>
                                                        <div class="single-input">

{{ form_row(registrationForm.countryCode,{'attr':{'placeholder':'Code pays'}}, {label: 'Code pays'} ) }}

                                                        </div>
                                                        <div class="file-custom">

                                                            {{ form_row(registrationForm.profile_picture,{'attr':{'class':'file-upload'}}) }}
                                                        </div>
                                                        <div class="single-input">

                                                            {{ form_row(registrationForm.roles,{label:'Role'}) }}
                                                        </div>
                                                        <div class="single-input">

{{ form_row(registrationForm.captcha,{'attr':{'placeholder':'Veuillez taper le code ci-dessus !'}}) }}


                                                        </div>
                                                        <div class="single-input">

                                                            {{ form_row(registrationForm.agreeTerms,{'attr':{'class':'checkmark'}}) }}
                                                        </div>

                                                    </div>
                                                                                                
                                                    <span class="btn-border w-100">
                                                                <button type="submit" class="cmn-btn w-100"> S'inscrire </button>
                                                            </span>
                                                </div>
                                                {{ form_end(registrationForm) }}


                                                <div class="bottom-area text-center">
<p>Déjà membre ?
	<a href="{{ path('app_login') }}"

                                                                             class="log-btn">Se connecter</a></p>
                                                </div>
                                               
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<!-- Login Registration end -->



	<div class="pos-content-container h-100 p-3" data-scrollbar="true" data-height="100%" >
    
	<div class="row gx-3">
    {% for product in products %}    
	<div class="col-xl-2 col-lg-3 col-md-4 col-sm-6 pb-3">
    
	<div class="card h-100">
	<div class="card-body h-100 p-1">
	<div class="pos-product">
    
    {% for image in product.images %}
	<div class="carousel-item {{ (loop.first) ? 'active' : '' }}">
			<img src="{{ asset('/uploads/products/' ~ image.name) }}" class="d-block w-100" alt="...">
	</div>
				{# <div><img src="" alt="Image" width="150">
                <a href="{{ path('product_delete_image', {id: image.id}) }}" data-delete data-token="{{ csrf_token('delete' ~ image.id) }}">Delete</a>
				</div> #}
    {% endfor %}
	<div class="info">
	<div class="title text-truncate">{{ product.name }}&reg;</div>
	<div class="desc text-truncate">Price: {{ product.price }}$</div>
	<div class="d-flex align-items-center mb-3">
	<div class="w-100px">Description: {{ product.description }}</div>
	
	</div>
	<div class="d-flex align-items-center mb-3">
	<div class="w-100px">Stock: {{ product.quantity }}</div>
	<div class="flex-1">
	
	</div>
	</div>
	<div>
    <a href="{{ path('product_edit', {'id': product.id}) }}", button class="btn btn-warning d-block mb-2">edit</a>
         <form method="post" action="{{ path('product_delete', {'id': product.id}) }}", onsubmit="return confirm('Are you sure you want to delete this item?');">
         <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ product.id) }}">
         <button class="btn btn-danger d-block mb-2 col-10" >Delete</button>
        </form>
	
	</div>
	</div>
	</div>
	</div>
    
